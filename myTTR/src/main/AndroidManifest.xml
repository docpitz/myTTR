<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.jmelzer.myttr"
          android:versionCode="8"
          android:versionName="0.11">
    <uses-sdk android:targetSdkVersion="21" android:maxSdkVersion="21" android:minSdkVersion="16"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!--<uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED"/>-->

    <application android:name="com.jmelzer.myttr.MyApplication" android:theme="@style/MyTheme"
                 android:label="@string/app_name" android:icon="@drawable/myttr">
        <activity android:name="com.jmelzer.myttr.activities.LoginActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.jmelzer.myttr.activities.HomeActivity"
                  android:label="@string/app_name" android:icon="@drawable/myttr"
                  android:parentActivityName="com.jmelzer.myttr.activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                       android:value="com.jmelzer.myttr.activities.HomeActivity"/>
        </activity>
        <activity android:name=".activities.TTRCalculatorActivity"
                  android:label="@string/app_name" android:icon="@drawable/myttr"
                  android:parentActivityName="com.jmelzer.myttr.activities.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                       android:value="com.jmelzer.myttr.activities.HomeActivity"/>
        </activity>
        <activity android:name="com.jmelzer.myttr.activities.PlayerDetailActivity"
                  android:label="@string/app_name" android:icon="@drawable/myttr"
                  android:parentActivityName=".activities.TTRCalculatorActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                       android:value=".activities.TTRCalculatorActivity"/>
        </activity>
        <activity android:name="com.jmelzer.myttr.activities.NextAppointmentsActivity"
                  android:label="@string/app_name" android:icon="@drawable/myttr"
                  android:parentActivityName=".activities.TTRCalculatorActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                       android:value=".activities.TTRCalculatorActivity"/>
        </activity>

        <activity android:name="com.jmelzer.myttr.activities.NextAppointmentPlayersActivity"
                  android:label="@string/app_name" android:icon="@drawable/myttr"
                  android:parentActivityName="com.jmelzer.myttr.activities.NextAppointmentsActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                       android:value="com.jmelzer.myttr.activities.NextAppointmentsActivity"/>
        </activity>
        <activity android:name="com.jmelzer.myttr.activities.ResultActivity"
                  android:label="@string/app_name" android:icon="@drawable/myttr"
                  android:parentActivityName=".activities.TTRCalculatorActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                       android:value=".activities.TTRCalculatorActivity"/>
        </activity>
        <activity android:name="com.jmelzer.myttr.activities.EnterTTRActivity"
                  android:label="@string/app_name" android:icon="@drawable/myttr"
                  android:parentActivityName="com.jmelzer.myttr.activities.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                       android:value="com.jmelzer.myttr.activities.HomeActivity"/>
        </activity>
        <activity android:name="com.jmelzer.myttr.activities.ClubListActivity"
                  android:label="@string/app_name" android:icon="@drawable/myttr"
                  android:parentActivityName="com.jmelzer.myttr.activities.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                       android:value="com.jmelzer.myttr.activities.HomeActivity"/>
        </activity>
        <activity android:name="com.jmelzer.myttr.activities.MySettingsActivity" android:label="@string/menu_settings"/>
        <activity android:name="com.jmelzer.myttr.activities.NewPointsActivity" android:label="@string/notifcation_result">
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                       android:value=".activities.TTRCalculatorActivity"/>
        </activity>
        <activity android:name="com.jmelzer.myttr.activities.EventsActivity"
                  android:label="@string/app_name" android:icon="@drawable/myttr"
                  android:parentActivityName="com.jmelzer.myttr.activities.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                       android:value="com.jmelzer.myttr.activities.HomeActivity"/>
        </activity>
        <activity android:name="com.jmelzer.myttr.activities.EventDetailActivity"
            android:label="@string/app_name" android:icon="@drawable/myttr"
            android:parentActivityName="com.jmelzer.myttr.activities.EventsActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jmelzer.myttr.activities.EventsActivity"/>
        </activity>
        <activity android:name="com.jmelzer.myttr.activities.SelectPlayerActivity"
            android:label="@string/app_name" android:icon="@drawable/myttr"
            android:parentActivityName="com.jmelzer.myttr.activities.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
            </intent-filter>
            <meta-data android:name="android.support.PARENT_ACTIVITY"
                android:value="com.jmelzer.myttr.activities.HomeActivity"/>
        </activity>
        <!--<service android:name=".logic.SyncManager" android:label="myTTR Background Sync"
                 android:icon="@drawable/myttr">
        </service>-->
    </application>


</manifest>
